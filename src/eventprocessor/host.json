{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond": 1,
                "excludedTypes": "Exception"
            }
        }
    },
    "extensions": {
        "eventHub": {
            "maxBatchSize": 10,
            "prefetchCount": 300,
            "batchCheckpointFrequency": 1
        }
    }
}